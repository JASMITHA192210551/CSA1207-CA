#include <stdio.h>

int main() {
    int numbers[] = {18, 45, 66, 23, 19, 10, 52, 71};
    int min = numbers[0];

    for (int i = 1; i < 8; i++) { // Directly iterate over the known count
        if (numbers[i] < min) {
            min = numbers[i];
        }
    }

    printf("The smallest number is: %d\n", min);
    return 0;
}
